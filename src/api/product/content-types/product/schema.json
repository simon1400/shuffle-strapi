{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "content": {
      "type": "dynamiczone",
      "components": [
        "content.short-artciles",
        "content.logo-carousel",
        "content.description-block",
        "content.cta-block",
        "content.content-item"
      ],
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "metaData": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta-data"
    },
    "Parameters": {
      "type": "component",
      "repeatable": true,
      "component": "content-comp.parameter",
      "required": true
    },
    "compares": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "benefits": {
      "type": "component",
      "repeatable": true,
      "component": "content-comp.description-block-comp",
      "required": true
    },
    "shortIcon": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "label": {
      "displayName": "label",
      "type": "component",
      "repeatable": true,
      "component": "content-comp.label"
    }
  }
}
