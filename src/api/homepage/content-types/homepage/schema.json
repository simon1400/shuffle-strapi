{
  "kind": "singleType",
  "collectionName": "homepages",
  "info": {
    "singularName": "homepage",
    "pluralName": "homepages",
    "displayName": "Homepage",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "dynamicContent": {
      "type": "dynamiczone",
      "components": [
        "content.cta-block",
        "content.short-artciles",
        "content.logo-carousel",
        "content.description-block"
      ],
      "required": true
    },
    "metaData": {
      "type": "component",
      "repeatable": false,
      "component": "seo.meta-data"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    }
  }
}
